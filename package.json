{
    "name": "@inseefrlab/lunatic-edt",
    "version": "0.10.6",
    "description": "Library of questionnaire components for EDT survey",
    "repository": {
        "type": "git",
        "url": "git://github.com/InseeFrLab/lunatic-edt.git"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "test": "jest --env=jsdom ",
        "build": "tsc",
        "lint:check": "eslint . --ext .ts,.tsx",
        "lint": "npm run lint:check -- --fix",
        "_format": "prettier \"**/*.{ts,tsx,json,md}\"",
        "format": "npm run _format -- --write",
        "format:check": "npm run _format -- --list-different",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook",
        "prepare": "husky install",
        "start": "tsc-watch"
    },
    "lint-staged": {
        "*.{ts,tsx}": [
            "eslint --fix"
        ],
        "*.{ts,tsx,json,md}": [
            "prettier --write"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged -v"
        }
    },
    "author": "u/InseeFrLab",
    "license": "MIT",
    "files": [
        "src/",
        "!src/test/",
        "dist/",
        "!dist/test/",
        "!dist/tsconfig.tsbuildinfo"
    ],
    "keywords": [
        "react",
        "component",
        "react-component",
        "library",
        "material-ui"
    ],
    "homepage": "https://github.com/InseeFrLab/lunatic-edt",
    "jest": {
        "transform": {
            "^.+\\.(ts|tsx|js|jsx)$": "ts-jest"
        },
        "testResultsProcessor": "jest-sonar-reporter",
        "testPathIgnorePatterns": [
            "/src/*/dist",
            "dist"
        ]
    },
    "devDependencies": {
        "@babel/core": "^7.18.13",
        "@commitlint/cli": "^17.0.3",
        "@commitlint/config-conventional": "^17.0.3",
        "@inseefr/lunatic": "2.0.6-v2",
        "@storybook/addon-a11y": "^6.5.12",
        "@storybook/addon-actions": "^6.5.12",
        "@storybook/addon-essentials": "^6.5.12",
        "@storybook/addon-interactions": "^6.5.12",
        "@storybook/addon-links": "^6.5.12",
        "@storybook/builder-webpack5": "^6.5.12",
        "@storybook/manager-webpack5": "^6.5.12",
        "@storybook/react": "^6.5.12",
        "@storybook/testing-library": "^0.0.13",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "@types/jest": "^29.0.3",
        "@types/node": "^16.4.9",
        "@types/react": "^18.0.17",
        "@types/uuid": "^8.3.4",
        "@typescript-eslint/eslint-plugin": "^4.28.5",
        "@typescript-eslint/parser": "^4.28.5",
        "babel-loader": "^8.2.5",
        "css-loader": "^6.7.1",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-storybook": "^0.6.4",
        "eslint-plugin-tss-unused-classes": "^0.0.4",
        "husky": "^8.0.1",
        "jest": "^29.0.3",
        "jest-environment-jsdom": "^29.3.1",
        "jest-sonar-reporter": "^2.0.0",
        "lint-staged": "^11.1.1",
        "prettier": "^2.3.2",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "sass-loader": "^13.0.2",
        "storybook-dark-mode": "^1.1.2",
        "ts-jest": "^29.0.3",
        "tsafe": "^1.0.1",
        "tsc-watch": "^5.0.3",
        "typescript": "^4.3.5"
    },
    "dependencies": {
        "@date-io/dayjs": "^2.16.0",
        "@emotion/react": "^11.10.4",
        "@emotion/styled": "^11.10.4",
        "@mui/material": "^5.10.9",
        "@mui/x-date-pickers": "^5.0.8",
        "@types/elasticlunrjs": "^1.0.0",
        "dayjs": "^1.11.5",
        "elasticlunrjs": "^1.0.1",
        "tss-react": "^4.0.0",
        "uuid": "^9.0.0"
    },
    "publishConfig": {
        "access": "public"
    }
}
