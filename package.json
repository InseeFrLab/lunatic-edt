{
    "name": "@inseefrlab/lunatic-edt",
    "version": "0.13.5",
    "description": "Library of questionnaire components for EDT survey",
    "repository": {
        "type": "git",
        "url": "git://github.com/InseeFrLab/lunatic-edt.git"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "test": "jest --env=jsdom ",
        "build": "tsc",
        "lint:check": "eslint . --ext .ts,.tsx",
        "lint": "npm run lint:check -- --fix",
        "_format": "prettier \"**/*.{ts,tsx,json,md}\"",
        "format": "npm run _format -- --write",
        "format:check": "npm run _format -- --list-different",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook",
        "prepare": "husky install",
        "start": "tsc-watch"
    },
    "lint-staged": {
        "*.{ts,tsx}": [
            "eslint --fix"
        ],
        "*.{ts,tsx,json,md}": [
            "prettier --write"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged -v"
        }
    },
    "author": "u/InseeFrLab",
    "license": "MIT",
    "files": [
        "src/",
        "!src/test/",
        "dist/",
        "!dist/test/",
        "!dist/tsconfig.tsbuildinfo"
    ],
    "keywords": [
        "react",
        "component",
        "react-component",
        "library",
        "material-ui"
    ],
    "homepage": "https://github.com/InseeFrLab/lunatic-edt",
    "jest": {
        "transform": {
            "^.+\\.(ts|tsx|js|jsx)$": "ts-jest"
        },
        "testResultsProcessor": "jest-sonar-reporter",
        "testPathIgnorePatterns": [
            "/src/*/dist",
            "dist"
        ]
    },
    "devDependencies": {
        "@babel/core": "^7.18.13",
        "@commitlint/cli": "^17.0.3",
        "@commitlint/config-conventional": "^17.0.3",
        "@inseefr/lunatic": "2.0.6-v2",
        "@storybook/addon-a11y": "^6.5.12",
        "@storybook/addon-actions": "^6.5.12",
        "@storybook/addon-essentials": "^6.5.12",
        "@storybook/addon-interactions": "^6.5.12",
        "@storybook/addon-links": "^6.5.12",
        "@storybook/builder-webpack5": "^6.5.12",
        "@storybook/manager-webpack5": "^6.5.12",
        "@storybook/react": "^6.5.12",
        "@storybook/testing-library": "^0.0.13",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "@types/jest": "^29.5.12",
        "@types/node": "^22.5.4",
        "@types/react": "^18.3.5",
        "@types/uuid": "^10.0.0",
        "@typescript-eslint/eslint-plugin": "^8.5.0",
        "@typescript-eslint/parser": "^8.5.0",
        "babel-loader": "^8.2.5",
        "css-loader": "^6.7.1",
        "eslint": "^9.10.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-storybook": "^0.8.0",
        "eslint-plugin-tss-unused-classes": "^1.0.2",
        "husky": "^9.1.5",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "jest-sonar-reporter": "^2.0.0",
        "lint-staged": "^15.2.10",
        "prettier": "^3.3.3",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "sass-loader": "^16.0.1",
        "storybook-dark-mode": "^1.1.2",
        "ts-jest": "^29.2.5",
        "tsafe": "^1.7.2",
        "tsc-watch": "^6.2.0",
        "typescript": "^5.6.2"
    },
    "dependencies": {
        "@date-io/dayjs": "^3.0.0",
        "@emotion/react": "^11.13.3",
        "@emotion/styled": "^11.13.0",
        "@mui/material": "^6.0.2",
        "@mui/x-date-pickers": "^7.16.0",
        "@types/elasticlunrjs": "^1.0.4",
        "dayjs": "^1.11.13",
        "elasticlunrjs": "^1.0.1",
        "tss-react": "^4.9.13",
        "uuid": "^10.0.0"
    },
    "publishConfig": {
        "access": "public"
    }
}
